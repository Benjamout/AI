<?xml version="1.0"?>
<robot name="simpleobstacle">
  <material name="red">
    <color rgba="0.8 0.1 0.1 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <!-- length x width x height -->
        <box size="0.7 0.5 0.2"/>
      </geometry>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <material name="red"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.7 0.5 0.2"/>
      </geometry>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0"/>
      <inertia
        ixx="0" ixy="0" ixz="0"
        iyy="0" iyz="0" izz="0"/>
    </inertial>
  </link>
</robot>
